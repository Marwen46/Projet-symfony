{% extends 'base.html.twig' %}
{% block title %}Ajouter Categorie {% endblock %} 
{% block body %}
<div class="container">
  <h1 class="mt-4 ml-2">Ajouter une nouvelle Categorie :</h1>
  {{ form_start(form) }}
  <div class="col-7">{{ form_row(form.nomCategorie) }}</div>
  {{ form_widget(form) }}
  <button class="ml-3 btn btn-primary mb-3">Ajouter</button>
  {{ form_end(form) }}
</div>
{% endblock %}
