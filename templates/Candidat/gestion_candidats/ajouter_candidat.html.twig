{% extends 'base.html.twig' %}
{% block title %}Ajouter candidat {% endblock %} 
{% block body %}
<div class="container   ">
  <h1 class="mt-4 ml-2">Ajouter un nouveau candidat :</h1>
  {{ form_start(form) }}
  <div class="col-7">{{ form_row(form.Nom) }}</div>
  <div class="col-7">{{ form_row(form.Prenom) }}</div>
  <div class="col-7">{{ form_row(form.Age) }}</div>
  <div class="col-7">{{ form_row(form.Email) }}</div>
  <div class="col-7">{{ form_row(form.categorie) }}</div>
  {{ form_widget(form) }}
  <button class="ml-3 btn btn-primary mb-3">Ajouter</button>
  {{ form_end(form) }}
</div>
{% endblock %}
