{% extends 'base.html.twig' %} {% block title %}Modifier candidat {%
endblock %} {% block body %}
<div class="container">
  <h1 class="mt-4 ml-2">Modifier un candidat :</h1>
  {{ form_start(form) }}
  <div class="col-6">{{ form_row(form.Nom) }}</div>
  <div class="col-6">{{ form_row(form.Prenom) }}</div>
  <div class="col-6">{{ form_row(form.Age) }}</div>
  <div class="col-6">{{ form_row(form.Email) }}</div>
  {{ form_widget(form) }}
  <button class="ml-3 btn btn-warning mb-3">Modifier</button>
  {{ form_end(form) }}
</div>
{% endblock %}
